<p style="color: green"><%= notice %></p>

<% content_for :title, "Tags" %>

<h1 data-e2e="tags-header">Tags</h1>

<div id="tags">
  <% @tags.each do |tag| %>
    <%= render tag %>
    <p>
      <%= link_to "Show this tag", tag,data: { e2e:"show_tag"} %>
    </p>
  <% end %>
</div>

<%= link_to "New tag", new_tag_path,data: { e2e:"new_tag"} %>
